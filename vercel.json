{
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 1 * * *"
    }
  ],
  "installCommand": "npm install -g pnpm@10.12.4 && pnpm install --frozen-lockfile"
}
